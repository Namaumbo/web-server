<?xml version='1.0' encoding='UTF-8'?>
<serverConfiguration>
        <httpEndpoint id="defaultHttpEndpoint" host="${env.HOST}" httpPort="8000" />
        <type>http</type>
        <port>8000</port>
        <ip></ip>
        <Logging>
            <errolog>
                <filename>"./Logs/error.log"</filename>
                <rollingsize>10M</rollingsize>
                <debuglevel>0</debuglevel>
                <loglevel>DEBUG</loglevel>
            </errolog>
            <accesslog>
                <filename>"./Logs/access.log"</filename>
                <rollingsize>10M</rollingsize>
            </accesslog>
            <serverlog>
                <filename>"./Logs/server.log"</filename>
                <rollingsize>10M</rollingsize>
            </serverlog>
        </Logging>
